#include <stdio.h>

int main (void)

{
    printf("ppppppppp\n");
    printf("    p   p\n");
    printf("    p   p\n");
    printf("    p   p\n");
    printf("     p p   \n");
    printf("\n");
    printf("  JJ\n");
    printf(" J\n");
    printf("J\n");
    printf(" J\n");
    printf("  JJJJJJJ\n");
    printf("\n");
    printf("DDDDDDDDD\n");
    printf("D       D\n");
    printf("D       D\n");
    printf(" D     D\n");
    printf("  DDDDD\n");

    return 0;
}


